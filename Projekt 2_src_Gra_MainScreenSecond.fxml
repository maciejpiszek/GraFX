<StackPane fx:id="stackPane" prefHeight="520.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gra.SelectScreenController">
    <children>
      <GridPane styleClass="Pomaranczka" stylesheets="@Style.css">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label fx:id="press" alignment="CENTER" contentDisplay="BOTTOM" prefHeight="76.0" prefWidth="480.0" text="PUSH TO START" textAlignment="JUSTIFY" textFill="#00cd00" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM">
               <font>
                  <Font name="Arial Bold" size="34.0" />
               </font>
               <effect>
                  <DropShadow />
               </effect>
            </Label>
         </children>
      </GridPane>
    </children>
</StackPane>
